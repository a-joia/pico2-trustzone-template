{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pico Debug (Cortex-Debug)",
            "cwd": "/home/ajoianeto/.pico-sdk/openocd/0.12.0+dev/scripts",
            "executable": "${workspaceRoot}/build_secure/secure/secure.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            // "serverpath": "${userHome}/.pico-sdk/openocd/0.12.0+dev/openocd.exe",
            "serverpath": "/home/ajoianeto/.pico-sdk/openocd/0.12.0+dev/openocd.exe",
            "gdbPath": "gdb-multiarch",
            "device": "RP2350",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2350.cfg"
            ],
            "svdFile": "${workspaceRoot}/drivers/rp2350/hardware_regs/RP2350.svd",
            "runToEntryPoint": "main",
            // Fix for no_flash binaries, where monitor reset halt doesn't do what is expected
            // Also works fine for flash binaries
            "showDevDebugOutput": "raw",
            "overrideLaunchCommands": [
                "monitor reset init",
                "load \"${workspaceRoot}/build_nonsecure/build/zephyr/zephyr.elf\"",
                "load \"${workspaceRoot}//build_secure/secure/secure.elf\"",
                "add-symbol-file \"${workspaceRoot}/build_nonsecure/build/zephyr/zephyr.elf\" 0x10100000",
                "set disassemble-next-line on",
                "set print asm-demangle on",
                "set print pretty on",
                "set style enabled off"
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ]
        },
        {
            "name": "Pico Debug (Cortex-Debug with external OpenOCD)",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build_secure/secure/secure.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "external",
            "gdbTarget": "host.docker.internal:50000",
            "gdbPath": "gdb-multiarch",
            "device": "RP2350",
            "svdFile": "${workspaceRoot}/drivers/rp2350/hardware_regs/RP2350.svd",
            "runToEntryPoint": "main",
            // Fix for no_flash binaries, where monitor reset halt doesn't do what is expected
            // Also works fine for flash binaries
            "overrideLaunchCommands": [
                "monitor reset init",
                "load \"${workspaceRoot}/build_secure/secure/secure.elf\"",
                "load \"${workspaceRoot}/build_nonsecure/nonsecure/nonsecure.elf\"",
                "add-symbol-file \"${workspaceRoot}/build_nonsecure/nonsecure/nonsecure.elf\" 0x10100000",
                "set disassemble-next-line on",
                "set print asm-demangle on",
                "set print pretty on",
                "set style enabled off"
            ]
        },
        {
            "name": "Pico Debug (C++ Debugger)",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${command:raspberry-pi-pico.launchTargetPath}",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb-multiarch",
            "miDebuggerServerAddress": "localhost:3333",
            "debugServerPath": "${workspaceRoot}/tools/openocd/openocd/0.12.0+dev/openocd",
            "debugServerArgs": "-f interface/cmsis-dap.cfg -f target/rp2350.cfg -c \"adapter speed 5000\"",
            "serverStarted": "Listening on port .* for gdb connections",
            "filterStderr": true,
            "hardwareBreakpoints": {
                "require": true,
                "limit": 4
            },
            "preLaunchTask": "Flash",
            "svdPath": "${workspaceRoot}/drivers/rp2350/hardware_regs/RP2350.svd"
        },
    ]
}



